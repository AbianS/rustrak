{
  "name": "@rustrak/test-sentry",
  "version": "0.1.0",
  "private": true,
  "description": "Test utility for sending Sentry events to Rustrak server",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "test-sentry": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "start": "tsx src/cli.ts",
    "test:all": "tsx src/cli.ts --all",
    "test:error": "tsx src/cli.ts --error",
    "test:unhandled": "tsx src/cli.ts --unhandled",
    "test:promise": "tsx src/cli.ts --promise",
    "test:message": "tsx src/cli.ts --message",
    "test:breadcrumbs": "tsx src/cli.ts --breadcrumbs",
    "test:context": "tsx src/cli.ts --context",
    "test:user": "tsx src/cli.ts --user",
    "test:tags": "tsx src/cli.ts --tags",
    "test:fingerprint": "tsx src/cli.ts --fingerprint",
    "test:flood": "tsx src/cli.ts --flood",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@sentry/node": "10.35.0"
  },
  "devDependencies": {
    "@types/node": "25.0.9",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  }
}
